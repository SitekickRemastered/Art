package com.digitalcrafts.media{		import flash.events.EventDispatcher;	import flash.events.Event;		public class Media extends EventDispatcher	{				private var _name:String;		public var isPlaying:Boolean;		private var _completeFunction:Function;		private var looping:Boolean = false;				public function Media($name:String, $completeFunction:Function = null)		{			name = $name;			completeFunction = $completeFunction;			isPlaying = false;		}		public function set name($name:String):void		{			_name = $name;		}		public function get name():String		{			return _name;		}		public function set completeFunction($completeFunction:Function):void		{			_completeFunction = $completeFunction;		}		public function get completeFunction():Function		{			return _completeFunction;		}		/*public function play():void{			isPlaying = true;		}		public function stop():void{			isPlaying = false;		}*/		public function onComplete(e:Event):void		{			isPlaying = false;			dispatchEvent(new Event(MediaEvent.MEDIA_COMPLETE));			if(_completeFunction != null)			{				_completeFunction();			}		}	}}